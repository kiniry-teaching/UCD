 cluster_chart HANDS
 	                         indexing
 	                         clauster:"HANDS;
 	                         created: "2008-11-26";
 	                         explanation "Players 5 CARDS which make up a VALUE."
 	                         part "1/1"
 	                         class RANK description "determines whether a HAND is better than another."
 	                         class ROYAL FLUSH description "highest HAND in poker-ten, jack, queen, king, ace all in the same SUITE."
 	                         class STRAIGHT FLUSH description "second HAND in poker-five CARDS in order, must be of the same SUITE."
 	                         class FOUR OF A KIND description "third HAND in poker-four of any CARD."
 	                         class FULL HOUSE description "fourth HAND in poker-three of one CARD and two of another."
 	                         class FLUSH description "fifth HAND in poker-five CARDS all with the same SUITE but not in order."
 	                         class STRAIGHT description "sixth HAND in poker-five CARDS all in order but not same SUITE."
 	                         class THREE OF A KIND description "seventh HAND in poker-three of one type of CARD."
 	                         class TWO PAIR description "eighth HAND in poker-two pairs of the same CARDS."
 	                         class ONE PAIR description "ninth HAND in poker-one pair of the same CARD"
 	                         class HIGH CARD description "lowest HAND in poker-if no other HANDS better highest VALUE CARD will win."
 	                         end
 	                         
class_chart RANK                 
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
	                          explanation "determines whether a HAND is better than another."
 	                          part "1/1"
	                          query
	                                          "What is your VALUE"
 	                          command
 	                                          "Be this RANK"
 	                          constraint
	                                          "remains unchanged"
 	                          end
 	                          
class_chart ROYAL FLUSH         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "highest HAND in poker-ten, jack, queen, king, ace all in the same SUITE."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "Must contain TEN - JACK - QUEEN - KING - ACE with all CARDS of the same SUITE"
 	                          end
 	                              
class_chart STRAIGHT FLUSH         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "five CARDS in order, must be of the same SUITE."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "Must contain five CARDS in ascending order of the same SUITE but not ten-ace."
 	                          end
 	                          
class_chart FOUR OF A KIND         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "four of any CARD."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "must be four CARDS of the same VALUE."
 	                          end
 	                          
class_chart FULL HOUSE         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "three of one CARD and two of another."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "must contain three CARDS of one VALUE and two CARDS of a differing VALUE."
 	                          end
 	                          
class_chart FLUSH         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "five CARDS all with the same SUITE but not in order."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "must be five CARDS of the same SUITE of varying VALUES."
 	                          end
 	                          
class_chart STRAIGHT         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "five CARDS all in order but not same SUITE."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "must have five CARDS of ascending VALUES not all the same SUITE ."
 	                          end
 	                          
class_chart THREE OF A KIND         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "three of one type of CARD."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "must be three CARDS of the same VALUE."
 	                          end
 	                          
class_chart TWO PAIRS         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "two pairs of the same CARDS."
 	                          part "1/1"
 	                          query
 	                                          "What SUITE are you" "What is your RANK."
 	                          command
 	                                          "Be this SUITE", "Have this RANK."
	                          constraint
	                                          "must be two pairs of equal VALUE CARDS with differing VALUES."
 	                          end
 	                          
class_chart ONE PAIR         
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "one pair of the same CARD."
 	                          part "1/1"
 	                          query
 	                                          "What SUIT are you" "What is your RANK."
 	                          command
 	                                          "Be this SUIT", "Have this RANK."
	                          constraint
	                                          "must be two CARDS of the same VALUE."
 	                          end
 	                          
class_chart HIGH CARD        
 	                          indexing
 	                          clauster: "HANDS";
 	                          created: "2008-11-26";
 	                          explanation "if no other hands better highest value card will win."
 	                          part "1/1"
 	                          query
 	                                          "What SUIT are you" "What is your RANK."
 	                          command
 	                                          "Be this SUIT", "Have this RANK."
	                          constraint
	                                          "must be no other HANDS of a greater VALUE."
 	                          end
 	                          
cluster_chart DECKS
 	                         indexing
 	                         clauster:"DECKS";
 	                         created: "2008-11-26";
 	                         explanation "all 52 CARDS including 4 different SUITES and each SUITE in ace to king"
 	                         part "1/1"
	                         class POKERDECK description  "all 52 CARDS including 4 different SUITES and each SUITE in ace to king."
	                         end
class_chart POKERDECK           
 	                         indexing
 	                         clauster: "DECKS";
 	                         created: "2008-11-26"; 	                          
 	                         explanation "A set of 52 POKERCARDS."
 	                         part "1/1"
 	                         query
 	                                          "How many CARDS do you have", "How many SUITES do you have."
 	                         command
 	                                          "have this amount of CARDS", "have these SUITES."
 	                         constraint
 	                                                  "Must have 52 POKER CARDS", "Must have four SUITES."
 	                         end
 	                         
cluster_chart SUITES 
 	                         indexing
 	                         clauster:"SUITES";
 	                         created: "2008-11-26";
