system_chart Cards
 	                        indexing
 	                        clauster: "POKER";
 	                        created: "2008-11-26";
 	                        explanation ""
 	                        part "1/1"
 	                        cluster CARDS description "used to play POKER."
 	                        cluster VALUES description "amount each CARD is worth."
	                        cluster HANDS description "Players five CARDS which make up a VALUE."
	                        cluster DECKS description "sets of 52 CARDS."
 	                        cluster SUITES description "can be one of four types of CARD."
 	                        end
 	                       
cluster_chart CARDS
                             indexing
 	                         clauster:"CARDS";
 	                         created: "2008-11-26";
 	                         explanation "used to play POKER."
 	                         part "1/1"
 	                         class RANK description "determines whether a HAND is better than another."
 	                         class SUITE description "can be one of four types- HEART, DIAMOND, SPADE, or CLUB."
 	                         class VALUE description "the amount each CARD is worth."
 	                         class ACE description "ace CARD has VALUE fourteen."
 	                         class TWO description "two CARD has VALUE two."
 	                         class THREE description "three CARD has VALUE three."
 	                         class FOUR description "four CARD has VALUE four."
 	                         class FIVE description "five CARD has VALUE five."
 	                         class SIX description "six CARD has VALUE six."
 	                         class SEVEN description "seven CARD has VALUE seven." 	                         
 	                         class EIGHT description "eight CARD has VALUE eight."
 	                         class NINE description "nine CARD has VALUE nine."
 	                         class TEN description "ten CARD has VALUE ten."
	                         class JACK description "jack FACE CARD has VALUE eleven."
 	                         class QUEEN description "queen FACE CARD has VALUE twelve."
 	                         class KING description "king FACE CARD has VALUE thirteen."
	                         end   
	                         
 class_chart RANK                 
 	                          indexing
 	                          clauster: "SUITS";
 	                          created: "2008-11-26";
	                          explanation "determines whether a HAND is better than another"
	                          part "1/1"
 	                       
 	                          query
 	                                                 "what is your RANK","are you the winning RANK", "are you a higher RANK", "are you a lower RANK", 	                                 
 	                          command
 	                                                 "be a RANK"
	                          constraint
 	                                                 "Can't change"
 	                          end 
 	                          
 class_chart SUITE                 
 	                          indexing
 	                          clauster: "SUITES";
 	                          created: "2008-11-26";
	                          explanation "can be one of four types"
	                          part "1/1"
 	                       
 	                          query
 	                                                 "are you a HEART","are you a DIAMOND","are you a SPADE","are you a CLUB"	                                 
 	                          command
 	                                                 "be a HEART","be a DIAMOND","be a SPADE","be a CLUB"
	                          constraint
 	                                                 "Can't change"
 	                          end
 	                          
 class_chart VALUE                 
 	                          indexing
 	                          clauster: "SUITES";
 	                          created: "2008-11-26";
	                          explanation "the amount each CARD is worth"
	                          part "1/1"
 	                       
 	                          query
 	                                                 "what number are you","what RANK are you"	                                 
 	                          command
 	                                                 "be a two","be a three","be a four","be a five","be a six","be a seven","be an eight","be a nine","be a ten","be an eleven","be a twelve","be a thirteen","be a fourteen"
	                          constraint
 	                                                 "must stay the same"
 	                          end
 	                          
 class_chart ACE                 
 	                          indexing
 	                          clauster: "SUITES";
 	                          created: "2008-11-26";
	                          explanation "highest RANK CARD in poker has VALUE fourteen"
	                          part "1/1"
 	                       
 	                          query
 	                                                 "what VALUE are you","what RANK are you","what COLOUR are you"	,"what SUITE are you"                                 
 	                          command
 	                                                 "be this VALUE","be this RANK","be this COLOUR","be this SUITE"
	                          constraint
 	                                                 "VALUE, RANK, COLOUR, SUITE remain the same "
 	                          end
 	                          
