cluster_chart VALUE
                         indexing
 	                         clauster:"CARDS";
 	                         created: "2008-11-26";
 	                         explanation "the amount each CARD is worth."
 	                         part "1/1"
 	                         class TWO description "CARD or HAND with the numerical VALUE of two in a POKER DECK."
 	                         class THREE description "CARD or HAND with the numerical VALUE of three in a POKER DECK."
	                         class FOUR description "CARD or HAND with the numerical VALUE of four in a POKER DECK."
 	                         class FIVE description "CARD or HAND with the numerical VALUE of five in a POKER DECK."
 	                         class SIX description "CARD or HAND with the numerical VALUE of six in a POKER DECK."
 	                         class SEVEN description "CARD or HAND with the numerical VALUE of seven in a POKER DECK."
 	                         class EIGHT description "CARD or HAND with the numerical VALUE of eight in a POKER DECK."
 	                         class NINE description "CARD or HAND with the numerical VALUE of nine in a POKER DECK."
 	                         class TEN description "CARD or HAND with the numerical VALUE of ten in a POKER DECK."
 	                         class ELEVEN description "CARD or HAND with the numerical VALUE of eleven in a POKER DECK."
 	                         class TWELVE description "CARD or HAND with the numerical VALUE of twelve in a POKER DECK."
 	                         class THIRTEEN description "CARD or HAND with the numerical VALUE of thirteen in a POKER DECK."
 	                         class FOURTEEN description "CARD or HAND with the numerical VALUE of fourteen in a POKER DECK."
 	                         end
 	              
class_chart TWO                 
 	                          indexing
 	                          clauster: "VALUES";
 	                          created: "2008-11-26";
 	                          explanation "lowest CARD has VALUE two."
 	                          part "1/1"   
 	                          query
 	                                                  "What VALUE are you", "What RANK are you". 
 	                          command
 	                                                  "Be this VALUE", "Be this RANK".
 	                          constraint
	                                                  "VALUE and RANK remain unchanged."
 	                          end
 	                          
